            <Dependency>qt5-base</Dependency>
            <Dependency>qt5-xmlpatterns</Dependency>
            <Dependency>qt5-webengine</Dependency>
            <Dependency>qt5-websockets</Dependency>
            <Dependency>qt5-datavis3d</Dependency>
