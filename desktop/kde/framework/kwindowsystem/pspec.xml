            <Dependency>xcb-util-devel</Dependency>
            <Dependency>xcb-util-keysyms-devel</Dependency>
            <Dependency versionFrom="5.52.0">extra-cmake-modules</Dependency>
        </BuildDependencies>
    </Source>

