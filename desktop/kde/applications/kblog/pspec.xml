            <Dependency>ksyndication-devel</Dependency>
            <Dependency>kxmlrpcclient-devel</Dependency>
            <Dependency>kcalcore-devel</Dependency>
            <Dependency>ksyndication-devel</Dependency>
        </BuildDependencies>
    </Source>

