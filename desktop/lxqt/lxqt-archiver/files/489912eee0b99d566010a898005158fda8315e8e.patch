+void MainWindow::dragEnterEvent(QDragEnterEvent *event) {
+    // If the drag is originated in this window, ignore it.
+    // This is needed when an archive contains another archive, as with deb packages.
+    if(event->source()) {
+        event->ignore();
+        return;
