+static bool isWithin256MB(const void* ptr, size_t length)
+{
+    const intptr_t start = reinterpret_cast<intptr_t>(ptr);
+    const intptr_t end = start + length - 1;
+    return (start & 0xf0000000) == (end & 0xf0000000);
