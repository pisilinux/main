+#endif // CPU(MIPS)
+
 void* OSAllocator::reserveUncommitted(size_t bytes, Usage usage, bool writable, bool executable, bool includesGuardPages)
 {
 #if OS(LINUX)
     UNUSED_PARAM(usage);
     UNUSED_PARAM(writable);
+#if !CPU(MIPS)
     UNUSED_PARAM(executable);
+#endif
     UNUSED_PARAM(includesGuardPages);
 
