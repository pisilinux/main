        </RuntimeDependencies>
        <Files>
            <Path fileType="library">/usr/lib32</Path>
        </Files>
    </Package>
    <Package>
        <Name>at-spi2-core-docs</Name>
        <IsA>data:doc</IsA>
        <Summary>at-spi2-core reference documents</Summary>
