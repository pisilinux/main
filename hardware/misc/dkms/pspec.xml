    <Package>
        <Name>dkms</Name>
        <RuntimeDependencies>
            <Dependency>gcc</Dependency>
            <Dependency>bash</Dependency>
            <Dependency>kmod</Dependency>
            <Dependency>make</Dependency>
            <Dependency>coreutils</Dependency>
        </RuntimeDependencies>
